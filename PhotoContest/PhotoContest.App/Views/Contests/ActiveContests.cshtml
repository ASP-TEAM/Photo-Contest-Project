@model IEnumerable<PhotoContest.App.Models.ViewModels.Contest.ContestViewModel>
@{
    ViewBag.Title = "Active Contests";
}

<h2>Active Contests</h2>
<div class="row">
    @foreach (var contest in Model)
    {
        <div class="col-md-4">
            @Html.Partial("_ActiveContest", contest);
        </div>
    }
</div>